{
  "name": "consumable-manager",
  "version": "1.0.0",
  "description": "An Electron application with Vue and TypeScript",
  "main": "./out/main/index.js",
  "author": "example.com",
  "homepage": "https://electron-vite.org",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint --cache .",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "dev:utf8": "chcp 65001 && electron-vite dev",
    "build": "electron-vite build",
    "build:fast": "cross-env NODE_ENV=production electron-vite build --mode production",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "npm run build:fast && electron-builder --dir",
    "build:win": "npm run build:fast && electron-builder --win --config.compression=store --config.directories.output=dist-win",
    "build:win:fast": "npm run build:fast && electron-builder --win --config.compression=store --config.nsis.oneClick=true --config.directories.output=dist-win",
    "build:win:dev": "npm run build:fast && electron-builder --win --config electron-builder-fast.yml --config.directories.output=dist-win",
    "build:mac": "npm run build:fast && electron-builder --mac",
    "build:linux": "npm run build:fast && electron-builder --linux",
    "dev:backend": "ts-node src/main/webServer.ts",
    "dev:web": "vite --config vite.config.web.ts",
    "build:backend": "tsc src/main/webServer.ts src/main/db-web.ts --outDir dist --target es2017 --module commonjs --esModuleInterop --allowSyntheticDefaultImports --strict --skipLibCheck --resolveJsonModule",
    "dev:full": "concurrently \"npm run dev:backend\" \"npm run dev:web\" --names \"backend,frontend\" --prefix-colors \"bgBlue.bold,bgMagenta.bold\"",
    "start:web": "concurrently \"npm run dev:backend\" \"npm run preview:web\" --names \"backend,frontend\" --prefix-colors \"bgBlue.bold,bgMagenta.bold\"",
    "build:web": "vite build --config vite.config.web.ts",
    "preview:web": "vite preview --config vite.config.web.ts"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/utils": "^4.0.0",
    "@tensorflow/tfjs": "^4.14.0",
    "axios": "^1.13.2",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.3",
    "body-parser": "^1.20.4",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.6",
    "csv-parser": "^3.2.0",
    "dayjs": "^1.11.19",
    "echarts": "^5.6.0",
    "electron-updater": "^6.3.9",
    "element-plus": "^2.10.2",
    "express": "^4.22.1",
    "express-session": "^1.18.2",
    "js-cookie": "^3.0.5",
    "mathjs": "^15.1.0",
    "md5": "^2.3.0",
    "ml-regression": "^6.3.0",
    "moment": "^2.30.1",
    "mssql": "^11.0.1",
    "node-pandas": "^1.0.5",
    "pinia": "^3.0.4",
    "prophet": "^0.1.1",
    "simple-statistics": "^7.8.8",
    "soap": "^1.2.1",
    "vue-echarts": "^7.0.3",
    "vue-router": "^4.6.3"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config-prettier": "3.0.0",
    "@electron-toolkit/eslint-config-ts": "^3.0.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/body-parser": "^1.19.6",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/express-session": "^1.18.2",
    "@types/mathjs": "^9.4.1",
    "@types/mssql": "^9.1.7",
    "@types/node": "^22.16.2",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vue/tsconfig": "^0.8.1",
    "concurrently": "^9.2.0",
    "cross-env": "^7.0.3",
    "electron": "^28.2.0",
    "electron-builder": "^22.14.13",
    "electron-vite": "^3.1.0",
    "eslint": "^9.24.0",
    "eslint-plugin-vue": "^10.0.0",
    "prettier": "^3.5.3",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.3",
    "typescript": "^5.9.3",
    "vite": "^6.2.6",
    "vue": "^3.5.27",
    "vue-eslint-parser": "^10.1.3",
    "vue-tsc": "^2.2.8",
    "webpack": "^5.99.9"
  },
  "build": {
    "appId": "com.electron.consumable-manager",
    "productName": "consumable-manager",
    "directories": {
      "output": "dist-win"
    },
    "electronDownload": {
      "mirror": "https://npmmirror.com/mirrors/electron/"
    },
    "compression": "normal",
    "asar": true,
    "files": [
      "!**/node_modules/moment-timezone/**",
      "!**/*.map",
      "!**/*.d.ts",
      "!src/**",
      "!*.md",
      "!*.log",
      "!.gitignore"
    ],
    "win": {
      "target": [
        {
          "target": "nsis"
        },
        "portable"
      ],
      "artifactName": "${productName}-${version}-${arch}.${ext}"
    },
    "nsis": {
      "oneClick": true,
      "allowToChangeInstallationDirectory": false,
      "perMachine": false,
      "useZip": false,
      "unicode": true,
      "deleteAppDataOnUninstall": false
    }
  }
}
